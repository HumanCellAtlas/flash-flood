# HCA DSS environment variables
#
# Source this file in your bash shell using "source environment".

# Resolve the location of this file and set FLASHFLOOD_HOME to the root
SOURCE="${BASH_SOURCE[0]}"
while [ -h "$SOURCE" ] ; do SOURCE="$(readlink "$SOURCE")"; done
export FLASHFLOOD_HOME="$(cd -P "$(dirname "$SOURCE")" && pwd)"


set -a
FLASHFLOOD_TEST_BUCKET="org-hca-dss-test"
set +a
